{
  "request": {
    "method": "PUT",
    "url": "/people/1",
    "bodyPatterns": [ {
      "equalToJson": "{\"first_name\":\"Bobby\"}",
      "ignoreArrayOrder" : true,
      "ignoreExtraElements" : true
    } ],
    "headers" : {
      "Authorization" : {
        "contains" : "i am a session token"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
